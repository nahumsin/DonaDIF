            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".CanastasBasicas"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".seleccionarFamilia"
            android:parentActivityName=".CanastasBasicas" />
        <activity android:name=".ActivityAnadirFamilia"
            android:parentActivityName=".MainActivity_Admin"/>
        <activity android:name=".ActivityListaFamilia_Donador"
